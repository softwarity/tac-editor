        "text": "VA CLD",
        "description": "Nuage de cendres volcaniques"
      },
      {
        "id": "va_eruption",
        "ref": "vaEruption",
        "text": "VA ERUPTION",
        "description": "Éruption volcanique"
      },
      {
        "id": "mt_prefix",
        "ref": "mt",
        "text": "MT",
        "description": "Préfixe montagne"
      },
      {
        "id": "volcano_name",
        "ref": "volcanoName",
        "provider": "sigmet-va-volcano-name",
        "pattern": "[A-Z][A-Z\\-\\s]+",
        "description": "Nom du volcan",
        "placeholder": "VOLCAN",
        "editable": {
          "start": 0,
          "end": 6,
          "pattern": "[A-Z][A-Z\\-\\s]+",
          "description": "Nom du volcan"
        }
      }
