{
  "name": "SIGMET WV",
  "version": "1.0.0",
  "description": "SIGMET VA: Cendres volcaniques",
  "extends": "sigmet",
  "category": "WV",
  "suggestions": {
    "declarations": [
      {
        "id": "sigmet_wv_full",
        "ref": "identifier",
        "provider": "sigmet-wv-fir-location-indicator",
        "suffix": " SIGMET",
