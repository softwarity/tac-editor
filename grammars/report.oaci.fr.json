      "pattern": "^\\/{5}(KT|MPS)$",
      "category": "unavailable",
      "description": "Vent non disponible système automatique (AUTO)"
    },
    "windVariation": {
      "pattern": "^\\d{3}V\\d{3}$",
      "category": "measurement",
      "description": "Direction du vent variable dndndnVdxdxdx variation ≥60° et <180° (15.5.3)",
