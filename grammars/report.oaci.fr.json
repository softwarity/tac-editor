      "placeholder": {
        "value": "00",
        "editable": [
          {
            "start": 0,
            "end": 2
          }
        ]
      }
    },
    "windDirectionSpeedGust": {
      "pattern": "^(VRB|\\d{3})P?\\d{2,3}GP?\\d{2,3}$",
