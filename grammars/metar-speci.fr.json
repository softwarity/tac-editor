        ], "required": false },
        { "token": "seaState", "required": false },
        { "token": "waveHeight", "required": false },
        { "token": "runwayState", "required": false, "repeat": true },
        { "oneOf": [
          { "token": "nosig" },
          { "sequence": [
            { "oneOf": [{ "token": "becmg" }, { "token": "tempo" }] },
            { "token": "trendTime", "required": false, "repeat": true, "max": 2 },
            { "token": "trendWind", "required": false },
            { "oneOf": [
              { "token": "trendVisibility", "required": false },
              { "token": "cavok", "required": false }
            ]},
            { "oneOf": [
              { "token": "trendWeather", "required": false, "repeat": true, "max": 3 },
              { "token": "nsw", "required": false }
            ]},
            { "token": "trendCloud", "required": false, "repeat": true, "max": 4 }
          ], "repeat": true }
        ], "required": false },
        { "sequence": [
          { "token": "remark", "required": true },
          { "token": "remarkContent", "required": false, "repeat": true }
        ], "required": false }
      ]}
    ]}
  ],
