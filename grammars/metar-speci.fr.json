{
  "name": "METAR/SPECI (Français)",
  "version": "2.0.0",
  "description": "FM 15 METAR / FM 16 SPECI - Message d'observation météorologique régulière/spéciale d'aérodrome (OMM-306 Vol I.1)",
  "reference": "OMM-No. 306, Manuel des codes, Volume I.1, Partie A, Section A",
  "identifiers": ["METAR", "SPECI"],
  "tokens": {
    "identifier": {
      "pattern": "^(METAR|SPECI)$",
      "style": "keyword",
      "description": "Identifiant du type de message (15.1.1)"
    },
    "correction": {
      "pattern": "^COR$",
      "style": "keyword",
      "description": "Indicateur de correction pour message corrigé"
    },
    "icao": {
      "pattern": "^[A-Z]{4}$",
      "style": "location",
      "description": "Indicateur d'emplacement OACI (15.2)"
    },
    "datetime": {
      "pattern": "^\\d{6}Z$",
      "style": "datetime",
      "description": "Jour et heure UTC JJHHmmZ (15.3)"
    },
    "nil": {
      "pattern": "^NIL$",
      "style": "keyword",
      "description": "Indicateur de message manquant"
    },
    "auto": {
      "pattern": "^AUTO$",
      "style": "keyword",
      "description": "Observation entièrement automatisée sans intervention humaine (15.4)"
    },
    "wind": {
      "pattern": "^(VRB|\\d{3})(\\d{2,3})(G\\d{2,3})?(KT|MPS|KMH)$|^00000(KT|MPS|KMH)$|^P(49MPS|99KT)$",
      "style": "wind",
      "description": "Vent de surface dddff[Gfmfm]{KT|MPS} (15.5)"
    },
    "windVariation": {
      "pattern": "^\\d{3}V\\d{3}$",
      "style": "wind",
      "description": "Direction du vent variable dndndnVdxdxdx variation ≥60° et <180° (15.5.3)"
    },
    "cavok": {
      "pattern": "^CAVOK$",
      "style": "visibility",
      "description": "Plafond et Visibilité OK - vis ≥10km, pas de nuage <1500m/MSA, pas de CB/TCU, pas de temps significatif (15.10)"
    },
    "visibility": {
      "pattern": "^\\d{4}$|^9999$|^0000$",
      "style": "visibility",
      "description": "Visibilité dominante VVVV en mètres (15.6)"
    },
    "directionalVisibility": {
      "pattern": "^\\d{4}(N|NE|E|SE|S|SW|W|NW)$",
      "style": "visibility",
      "description": "Visibilité minimale VNVNVNVNDv avec direction (15.6.2)"
    },
    "rvr": {
      "pattern": "^R\\d{2}[LCR]?\\/[PM]?\\d{4}(V[PM]?\\d{4})?[UDN]?$",
      "style": "visibility",
      "description": "Portée visuelle de piste RDRDR/VRVRVRVRi avec préfixe P/M et tendance U/D/N (15.7)"
    },
    "weather": {
      "pattern": "^[-+]?(VC)?(MI|PR|BC|DR|BL|SH|TS|FZ)?(DZ|RA|SN|SG|IC|PL|GR|GS|UP)*(BR|FG|FU|VA|DU|SA|HZ|PY)?(PO|SQ|FC|SS|DS)?$",
      "style": "weather",
      "description": "Temps présent w'w' - intensité/proximité + descripteur + phénomènes (15.8, Table de code 4678)"
    },
    "weatherSpecial": {
      "pattern": "^(VCSH|VCTS|VCFG|VCPO|VCFC|VCDS|VCSS|VCBLDU|VCBLSA|VCBLSN|VCVA)$",
      "style": "weather",
      "description": "Phénomènes météorologiques au voisinage VC (15.8.10)"
    },
    "noWeather": {
      "pattern": "^\\/{2}$",
      "style": "weather",
      "description": "Temps présent non observable par système automatique (15.8.19)"
    },
    "cloud": {
      "pattern": "^(FEW|SCT|BKN|OVC)(\\d{3})(CB|TCU|\\/\\/\\/)?$",
      "style": "cloud",
      "description": "Couche nuageuse NsNsNshshshs - quantité + hauteur en 100s pieds + type (15.9.1)"
    },
    "cloudUnknown": {
      "pattern": "^(FEW|SCT|BKN|OVC)\\/\\/\\/(CB|TCU|\\/\\/\\/)?$",
      "style": "cloud",
      "description": "Couche nuageuse avec hauteur inconnue (AUTO) (15.9.1.6)"
    },
    "verticalVisibility": {
      "pattern": "^VV(\\d{3}|\\/\\/\\/)$",
      "style": "cloud",
      "description": "Visibilité verticale VVhshshs en 100s pieds quand ciel obscurci (15.9.2)"
    },
    "nsc": {
      "pattern": "^NSC$",
      "style": "cloud",
      "description": "Pas de nuage significatif - pas de nuage <1500m/MSA, pas de CB/TCU (15.9.1.1)"
    },
    "ncd": {
      "pattern": "^NCD$",
      "style": "cloud",
      "description": "Pas de nuage détecté par le système d'observation automatique (15.9.1.1)"
    },
    "skyClear": {
      "pattern": "^(SKC|CLR)$",
      "style": "cloud",
      "description": "Ciel clair (usage régional)"
    },
    "temperature": {
      "pattern": "^M?\\d{2}\\/M?\\d{2}$",
      "style": "temperature",
      "description": "Température/Point de rosée T'T'/T'd T'd en °C, M pour moins (15.11)"
    },
    "pressure": {
      "pattern": "^Q\\d{4}$",
      "style": "pressure",
      "description": "Pression QNH QPHPHPHPH en hPa (15.12)"
    },
    "pressureInches": {
      "pattern": "^A\\d{4}$",
      "style": "pressure",
      "description": "Calage altimétrique en pouces de mercure (régional - Amérique du Nord)"
    },
    "recentWeather": {
      "pattern": "^RE(MI|PR|BC|DR|BL|SH|TS|FZ)?(DZ|RA|SN|SG|IC|PL|GR|GS|UP)*(BR|FG|FU|VA|DU|SA|HZ|PY)?(PO|SQ|FC|SS|DS)?$",
      "style": "weather",
      "description": "Temps récent REw'w' depuis le dernier message ou la dernière heure (15.13.2)"
    },
    "windshear": {
      "pattern": "^WS$",
      "style": "warning",
      "description": "Indicateur de cisaillement de vent (15.13.3)"
    },
    "windshearRunway": {
      "pattern": "^R\\d{2}[LCR]?$",
      "style": "warning",
      "description": "Indicatif de piste cisaillement de vent"
    },
    "windshearAllRwy": {
      "pattern": "^ALL$",
      "style": "warning",
      "description": "Indicateur cisaillement de vent ALL RWY"
    },
    "rwy": {
      "pattern": "^RWY$",
      "style": "warning",
      "description": "Mot-clé RWY pour WS ALL RWY"
    },
    "seaState": {
      "pattern": "^W(M?\\d{2})\\/S[0-9S]$",
      "style": "supplementary",
      "description": "Température et état de la mer WTsTs/SS' (15.13.5)"
    },
    "waveHeight": {
      "pattern": "^W(M?\\d{2})\\/H\\d{3}$",
      "style": "supplementary",
      "description": "Température de la mer et hauteur significative des vagues WTsTs/HHsHsHs (15.13.5)"
    },
    "runwayState": {
      "pattern": "^R\\d{2}[LCR]?\\/([0-9\\/]{6}|CLRD\\/\\/|SNOCLO)$",
      "style": "supplementary",
      "description": "État de la piste RDRDR/ERCReReRBRBR (15.13.6)"
    },
    "nosig": {
      "pattern": "^NOSIG$",
      "style": "trend",
      "description": "Pas de changement significatif prévu dans les 2 prochaines heures (15.14)"
    },
    "becmg": {
      "pattern": "^BECMG$",
      "style": "trend",
      "description": "Devenant - changement prévu à taux régulier/irrégulier (15.14.4)"
    },
    "tempo": {
      "pattern": "^TEMPO$",
      "style": "trend",
      "description": "Fluctuations temporaires <1h chacune, <50% de la période (15.14.7)"
    },
    "trendTime": {
      "pattern": "^(FM|TL|AT)\\d{4}$",
      "style": "datetime",
      "description": "Indicateur de temps tendance FM/TL/AT + HHmm (15.14.3)"
    },
    "trendWind": {
      "pattern": "^(VRB|\\d{3})(\\d{2,3})(G\\d{2,3})?(KT|MPS|KMH)$|^00000(KT|MPS|KMH)$",
      "style": "wind",
      "description": "Vent prévu tendance"
    },
    "trendVisibility": {
      "pattern": "^\\d{4}$|^9999$|^CAVOK$",
      "style": "visibility",
      "description": "Visibilité prévue tendance"
    },
    "trendWeather": {
      "pattern": "^[-+]?(VC)?(MI|PR|BC|DR|BL|SH|TS|FZ)?(DZ|RA|SN|SG|IC|PL|GR|GS|UP)*(BR|FG|FU|VA|DU|SA|HZ|PY)?(PO|SQ|FC|SS|DS)?$",
      "style": "weather",
      "description": "Temps prévu tendance"
    },
    "nsw": {
      "pattern": "^NSW$",
      "style": "weather",
      "description": "Pas de temps significatif - fin des phénomènes météo dans la tendance (15.14.12)"
    },
    "trendCloud": {
      "pattern": "^(FEW|SCT|BKN|OVC)(\\d{3})(CB|TCU)?$|^VV(\\d{3}|\\/\\/\\/)$|^NSC$",
      "style": "cloud",
      "description": "Nuages prévus tendance"
    },
    "remark": {
      "pattern": "^RMK$",
      "style": "remark",
      "description": "Indicateur de section remarques"
    },
    "remarkContent": {
      "pattern": "^[A-Z0-9\\/]+$",
      "style": "remark",
      "description": "Contenu des remarques (usage national)"
    }
  },
