{
  "name": "SWX",
  "version": "1.0.0",
  "description": "SWX: Avis de meteorologie de l'espace",
  "identifier": "SWX ADVISORY",
  "templateMode": true,
  "template": {
    "labelColumnWidth": 20,
    "fields": [
      {
        "label": "DTG:",
        "labelType": "dtgLabel",
        "valueType": "dtgValue",
        "required": true,
        "placeholder": "20161108/0100Z"
      },
      {
        "label": "SWXC:",
        "labelType": "swxcLabel",
        "valueType": "swxcValue",
        "required": true,
        "placeholder": "DONLON"
      },
      {
        "label": "ADVISORY NR:",
        "labelType": "advisoryNrLabel",
        "valueType": "advisoryNrValue",
        "required": true,
        "placeholder": "2016/2"
      },
      {
        "label": "NR RPLC:",
        "labelType": "nrRplcLabel",
        "valueType": "nrRplcValue",
        "required": false,
        "placeholder": "2016/1"
      },
      {
        "label": "SWX EFFECT:",
        "labelType": "swxEffectLabel",
        "valueType": "swxEffectValue",
        "required": true,
        "placeholder": "HF COM MOD"
      },
      {
        "label": "OBS SWX:",
        "labelType": "obsSwxLabel",
        "valueType": "obsSwxValue",
        "required": true,
        "placeholder": "08/0100Z HNH HSH E18000 - W18000"
      },
      {
        "label": "FCST SWX +6 HR:",
        "labelType": "fcstSwx6Label",
        "valueType": "fcstSwxValue",
        "required": true,
        "placeholder": "08/0700Z HNH HSH E18000 - W18000"
      },
      {
        "label": "FCST SWX +12 HR:",
        "labelType": "fcstSwx12Label",
        "valueType": "fcstSwxValue",
        "required": true,
        "placeholder": "08/1300Z HNH HSH E18000 - W18000"
      },
      {
        "label": "FCST SWX +18 HR:",
        "labelType": "fcstSwx18Label",
        "valueType": "fcstSwxValue",
        "required": true,
        "placeholder": "08/1900Z HNH HSH E18000 - W18000"
      },
      {
        "label": "FCST SWX +24 HR:",
        "labelType": "fcstSwx24Label",
        "valueType": "fcstSwxValue",
        "required": true,
        "placeholder": "09/0100Z NO SWX EXP"
      },
      {
        "label": "RMK:",
        "labelType": "rmkLabel",
        "valueType": "rmkValue",
        "required": true,
        "placeholder": "NIL"
      },
      {
        "label": "NXT ADVISORY:",
        "labelType": "nxtAdvisoryLabel",
        "valueType": "nxtAdvisoryValue",
        "required": true,
        "placeholder": "20161108/0700Z"
      }
    ]
  },
  "tokens": {
    "identifier": {
      "pattern": "^SWX ADVISORY$",
      "style": "keyword",
      "description": "Identifiant du type de message"
    },
    "statusLabel": {
      "pattern": "^STATUS:$",
      "style": "label",
      "description": "Etiquette indicateur de statut"
    },
    "statusValue": {
      "pattern": "^(TEST|EXER)$",
      "style": "keyword",
      "description": "Indicateur de test ou exercice"
    },
    "dtgLabel": {
      "pattern": "^DTG:$",
      "style": "label",
      "description": "Etiquette groupe date/heure"
    },
    "dtgValue": {
      "pattern": "^\\d{8}/\\d{4}Z$",
      "style": "datetime",
      "description": "Date et heure d'origine AAAAMMJJ/HHmmZ"
    },
    "swxcLabel": {
      "pattern": "^SWXC:$",
      "style": "label",
      "description": "Etiquette nom du centre de meteorologie spatiale"
    },
    "swxcValue": {
      "pattern": "^[A-Z][A-Z\\s\\-]{0,30}[A-Z]$|^[A-Z]+$",
      "style": "location",
      "description": "Nom du centre de meteorologie spatiale"
    },
    "advisoryNrLabel": {
      "pattern": "^ADVISORY NR:$",
      "style": "label",
      "description": "Etiquette numero d'avis"
    },
    "advisoryNrValue": {
      "pattern": "^\\d{4}/\\d{1,4}$",
      "style": "value",
      "description": "Numero d'avis AAAA/n[nnn]"
    },
    "nrRplcLabel": {
      "pattern": "^NR RPLC:$",
      "style": "label",
      "description": "Etiquette numero remplace"
    },
    "nrRplcValue": {
      "pattern": "^\\d{4}/\\d{1,4}$",
      "style": "value",
      "description": "Numero de l'avis remplace AAAA/n[nnn]"
    },
    "swxEffectLabel": {
      "pattern": "^SWX EFFECT:$",
      "style": "label",
      "description": "Etiquette effet meteorologie spatiale"
    },
    "swxEffectValue": {
      "pattern": "^(HF COM (MOD|SEV)|SATCOM (MOD|SEV)|GNSS (MOD|SEV)|RADIATION (MOD|SEV))( AND (HF COM (MOD|SEV)|SATCOM (MOD|SEV)|GNSS (MOD|SEV)|RADIATION (MOD|SEV)))*$",
      "style": "phenomenon",
      "description": "Effet et intensite de la meteorologie spatiale"
    },
    "obsSwxLabel": {
      "pattern": "^OBS SWX:$",
      "style": "label",
      "description": "Etiquette meteorologie spatiale observee"
    },
    "fcstSwxLabel": {
      "pattern": "^FCST SWX:$",
      "style": "label",
      "description": "Etiquette meteorologie spatiale prevue"
    },
    "fcstSwx6Label": {
      "pattern": "^FCST SWX \\+6 HR:$",
      "style": "label",
      "description": "Etiquette prevision +6 heures"
    },
    "fcstSwx12Label": {
      "pattern": "^FCST SWX \\+12 HR:$",
      "style": "label",
      "description": "Etiquette prevision +12 heures"
    },
    "fcstSwx18Label": {
      "pattern": "^FCST SWX \\+18 HR:$",
      "style": "label",
      "description": "Etiquette prevision +18 heures"
    },
    "fcstSwx24Label": {
      "pattern": "^FCST SWX \\+24 HR:$",
      "style": "label",
      "description": "Etiquette prevision +24 heures"
    },
    "obsSwxValue": {
      "pattern": "^\\d{2}/\\d{4}Z\\s.+$",
      "style": "value",
      "description": "Etendue de la meteorologie spatiale observee"
    },
    "fcstSwxValue": {
      "pattern": "^(\\d{2}/\\d{4}Z\\s.+|NO SWX EXP|NOT AVBL)$",
      "style": "value",
      "description": "Etendue de la meteorologie spatiale prevue"
    },
    "swxDtg": {
      "pattern": "^\\d{2}/\\d{4}Z$",
      "style": "datetime",
      "description": "Jour et heure JJ/HHmmZ"
    },
    "daylightSide": {
      "pattern": "^DAYLIGHT SIDE$",
      "style": "location",
      "description": "Cote eclaire de la Terre"
    },
    "latitudeBand": {
      "pattern": "^(HNH|MNH|EQN|EQS|MSH|HSH)$",
      "style": "location",
      "description": "Bande de latitude (HNH=Nord eleve, MNH=Nord moyen, EQN=Equat Nord, EQS=Equat Sud, MSH=Sud moyen, HSH=Sud eleve)"
    },
    "longitudeRange": {
      "pattern": "^[EW]\\d{3,5}\\s*-\\s*[EW]\\d{3,5}$",
      "style": "location",
      "description": "Plage de longitude"
    },
    "flightLevel": {
      "pattern": "^ABV FL\\d{3}$|^FL\\d{3}\\s*-\\s*\\d{3}$",
      "style": "value",
      "description": "Niveau de vol ou plage"
    },
    "noSwxExp": {
      "pattern": "^NO SWX EXP$",
      "style": "keyword",
      "description": "Aucune meteorologie spatiale attendue"
    },
    "notAvbl": {
      "pattern": "^NOT AVBL$",
      "style": "keyword",
      "description": "Prevision non disponible"
    },
    "rmkLabel": {
      "pattern": "^RMK:$",
      "style": "label",
      "description": "Etiquette remarques"
    },
    "rmkValue": {
      "pattern": "^([A-Z0-9][A-Z0-9\\-\\.\\/\\(\\)\\,\\+\\s]{0,254}|\\.{2,3}[A-Z][A-Z0-9\\/]+)$|^NIL$",
      "style": "value",
      "description": "Remarques (texte libre jusqu'a 256 caracteres) ou NIL"
    },
    "nxtAdvisoryLabel": {
      "pattern": "^NXT ADVISORY:$",
      "style": "label",
      "description": "Etiquette prochain avis"
    },
    "nxtAdvisoryValue": {
      "pattern": "^\\d{8}/\\d{4}Z$",
      "style": "datetime",
      "description": "Date/heure du prochain avis AAAAMMJJ/HHmmZ"
    },
    "nxtAdvisoryNoFurther": {
      "pattern": "^NO FURTHER ADVISORIES$",
      "style": "keyword",
      "description": "Aucun autre avis attendu"
    },
    "nxtAdvisoryWillBeIssuedBy": {
      "pattern": "^WILL BE ISSUED BY \\d{8}/\\d{4}Z$",
      "style": "datetime",
      "description": "L'avis sera emis avant date/heure"
    }
  },
