      "category": "keyword",
      "description": "Pas de changement"
    },
    "fcstAt": {
      "pattern": "^FCST AT$",
      "category": "keyword",
      "description": "Prévu à l'heure"
    },
    "fcstTime": {
      "pattern": "^\\d{4}Z$",
      "category": "datetime",
      "description": "Heure de prévision HHmmZ"
    }
  },
