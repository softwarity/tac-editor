    "speedKmh": {
      "pattern": "^\\d{1,3}KMH$",
      "category": "measurement",
      "description": "Vitesse en km/h"
    },
    "speedKt": {
      "pattern": "^\\d{1,3}KT$",
      "category": "measurement",
