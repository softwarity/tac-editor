{
  "name": "MET Base",
  "version": "1.0.0",
  "description": "MET: Grammaire de base pour SIGMET/AIRMET",
  "tokens": {
    "firId": {
      "pattern": "^[A-Z]{4}$",
      "category": "location",
      "description": "Indicateur OACI de la FIR/CTA"
    },
    "firIdArea": {
      "pattern": "^[A-Z]{4}$",
      "category": "location",
      "description": "Indicateur OACI de la zone FIR/CTA affectée"
    },
    "sequenceNumber": {
      "pattern": "^[A-Z]?\\d{1,3}$",
      "category": "identifier",
      "description": "Numéro de séquence [A-Z]n[n][n]"
    },
    "valid": {
      "pattern": "^VALID$",
      "category": "keyword",
      "description": "Mot-clé validité"
    },
    "validityPeriod": {
      "pattern": "^\\d{6}/\\d{6}$",
      "category": "datetime",
      "description": "Période de validité JJHHmm/JJHHmm"
    },
    "mwoId": {
      "pattern": "^[A-Z]{4}-$",
      "category": "location",
      "description": "Indicateur MWO avec tiret",
      "placeholder": {
        "value": "AAAA-",
        "editable": [{ "start": 0, "end": 4 }]
      }
    },
    "firName": {
      "pattern": "^[A-Z][A-Z\\s]{1,20}$",
      "category": "location",
      "description": "Nom de la FIR/CTA",
      "placeholder": {
        "value": "NOM",
        "editable": [{ "start": 0, "end": 3 }]
      }
    },
    "fir": {
      "pattern": "^FIR$",
      "category": "keyword",
      "description": "Région d'information de vol"
    },
    "uir": {
      "pattern": "^UIR$",
      "category": "keyword",
      "description": "Région supérieure d'information"
    },
    "firUir": {
      "pattern": "^FIR/UIR$",
      "category": "keyword",
      "description": "FIR / UIR"
    },
    "cta": {
      "pattern": "^CTA$",
      "category": "keyword",
      "description": "Région de contrôle"
    },
    "firNumber": {
      "pattern": "^/\\d{1,2}$",
      "category": "identifier",
      "description": "Numéro de subdivision FIR"
    },
    "statusTest": {
      "pattern": "^TEST$",
      "category": "status",
      "description": "Indicateur message de test"
    },
    "statusExer": {
      "pattern": "^EXER$",
      "category": "status",
      "description": "Indicateur message d'exercice"
    },
    "cnl": {
      "pattern": "^CNL$",
      "category": "keyword",
      "description": "Mot-clé annulation"
    },
    "cnlSequence": {
      "pattern": "^[A-Z]?\\d{1,3}$",
      "category": "identifier",
      "description": "Numéro de séquence du message annulé"
    },
    "cnlValidity": {
      "pattern": "^\\d{6}/\\d{6}$",
      "category": "datetime",
      "description": "Période de validité du message annulé"
    },
    "obs": {
      "pattern": "^OBS$",
      "category": "keyword",
      "description": "Observé"
    },
    "fcst": {
      "pattern": "^FCST$",
      "category": "keyword",
      "description": "Prévu"
    },
    "at": {
      "pattern": "^AT$",
      "category": "keyword",
      "description": "Mot-clé AT"
    },
    "obsTime": {
      "pattern": "^\\d{4}Z$",
      "category": "datetime",
      "description": "Heure d'observation/prévision HHmmZ"
    },
    "coordinate": {
      "pattern": "^[NS]\\d{2,4}\\s[EW]\\d{3,5}$",
      "category": "location",
      "description": "Coordonnée géographique Nnn[nn] Ennn[nn] ou Snn[nn] Wnnn[nn]"
    },
    "coordinateSimple": {
      "pattern": "^[NS]\\d{2,4}$",
      "category": "location",
      "description": "Coordonnée latitude simple Nnn[nn] ou Snn[nn]"
    },
    "coordinateLon": {
      "pattern": "^[EW]\\d{3,5}$",
      "category": "location",
      "description": "Coordonnée longitude simple Ennn[nn] ou Wnnn[nn]"
    },
    "coordinateSeparator": {
      "pattern": "^-$",
      "category": "keyword",
      "description": "Séparateur de coordonnées"
    },
    "nOf": {
      "pattern": "^N OF$",
      "category": "keyword",
      "description": "Au nord de"
    },
    "sOf": {
      "pattern": "^S OF$",
      "category": "keyword",
      "description": "Au sud de"
    },
    "eOf": {
      "pattern": "^E OF$",
      "category": "keyword",
      "description": "À l'est de"
    },
    "wOf": {
      "pattern": "^W OF$",
      "category": "keyword",
      "description": "À l'ouest de"
    },
    "and": {
      "pattern": "^AND$",
      "category": "keyword",
      "description": "Conjonction ET"
    },
    "nOfLine": {
      "pattern": "^N OF LINE$",
      "category": "keyword",
      "description": "Au nord de la ligne"
    },
    "neOfLine": {
      "pattern": "^NE OF LINE$",
      "category": "keyword",
      "description": "Au nord-est de la ligne"
    },
    "eOfLine": {
      "pattern": "^E OF LINE$",
      "category": "keyword",
      "description": "À l'est de la ligne"
    },
    "seOfLine": {
      "pattern": "^SE OF LINE$",
      "category": "keyword",
      "description": "Au sud-est de la ligne"
    },
    "sOfLine": {
      "pattern": "^S OF LINE$",
      "category": "keyword",
      "description": "Au sud de la ligne"
    },
    "swOfLine": {
      "pattern": "^SW OF LINE$",
      "category": "keyword",
      "description": "Au sud-ouest de la ligne"
    },
    "wOfLine": {
      "pattern": "^W OF LINE$",
      "category": "keyword",
      "description": "À l'ouest de la ligne"
    },
    "nwOfLine": {
      "pattern": "^NW OF LINE$",
      "category": "keyword",
      "description": "Au nord-ouest de la ligne"
    },
    "wi": {
      "pattern": "^WI$",
      "category": "keyword",
      "description": "À l'intérieur (polygone)"
    },
    "aprxWideLine": {
      "pattern": "^APRX \\d{1,3}KM WID LINE BTN$",
      "category": "keyword",
      "description": "Largeur approximative du couloir en km"
    },
    "aprxWideLineNm": {
      "pattern": "^APRX \\d{1,3}NM WID LINE BTN$",
      "category": "keyword",
      "description": "Largeur approximative du couloir en nm"
    },
    "entireFir": {
      "pattern": "^ENTIRE FIR$",
      "category": "keyword",
      "description": "FIR entière"
    },
    "entireUir": {
      "pattern": "^ENTIRE UIR$",
      "category": "keyword",
      "description": "UIR entière"
    },
    "entireFirUir": {
      "pattern": "^ENTIRE FIR/UIR$",
      "category": "keyword",
      "description": "FIR/UIR entière"
    },
    "entireCta": {
      "pattern": "^ENTIRE CTA$",
      "category": "keyword",
      "description": "CTA entière"
    },
    "flightLevel": {
      "pattern": "^FL\\d{3}$",
      "category": "measurement",
      "description": "Niveau de vol FLnnn"
    },
    "sfcFl": {
      "pattern": "^SFC/FL\\d{3}$",
      "category": "measurement",
      "description": "Surface au niveau de vol"
    },
    "sfcAlt": {
      "pattern": "^SFC/\\d{4,5}(M|FT)$",
      "category": "measurement",
      "description": "Surface à l'altitude en mètres ou pieds"
    },
    "flRange": {
      "pattern": "^FL\\d{3}/\\d{3}$",
      "category": "measurement",
      "description": "Plage de niveaux de vol FLnnn/nnn"
    },
    "topFl": {
      "pattern": "^TOP FL\\d{3}$",
      "category": "measurement",
      "description": "Sommet niveau de vol"
    },
    "topAbvFl": {
      "pattern": "^TOP ABV FL\\d{3}$",
      "category": "measurement",
      "description": "Sommet au-dessus du niveau de vol"
    },
    "abvFl": {
      "pattern": "^ABV FL\\d{3}$",
      "category": "measurement",
      "description": "Au-dessus du niveau de vol"
    },
    "altRange": {
      "pattern": "^\\d{4,5}/\\d{4,5}(M|FT)$",
      "category": "measurement",
      "description": "Plage d'altitude en mètres ou pieds"
    },
    "altToFl": {
      "pattern": "^\\d{4,5}(M|FT)/FL\\d{3}$",
      "category": "measurement",
      "description": "Altitude au niveau de vol"
    },
    "movN": {
      "pattern": "^MOV N$",
      "category": "status",
      "description": "Se déplaçant vers le nord"
    },
    "movNne": {
      "pattern": "^MOV NNE$",
      "category": "status",
      "description": "Se déplaçant vers le nord-nord-est"
    },
    "movNe": {
      "pattern": "^MOV NE$",
      "category": "status",
