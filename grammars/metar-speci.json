{
  "name": "METAR/SPECI",
  "version": "2.0.0",
  "description": "FM 15 METAR / FM 16 SPECI - Aerodrome routine/special meteorological report (WMO-306 Vol I.1)",
  "reference": "WMO-No. 306, Manual on Codes, Volume I.1, Part A, Section A",
  "identifiers": ["METAR", "SPECI"],
  "tokens": {
    "identifier": {
      "pattern": "^(METAR|SPECI)$",
      "style": "keyword",
      "description": "Message type identifier (15.1.1)"
    },
    "correction": {
      "pattern": "^COR$",
      "style": "keyword",
      "description": "Correction indicator for corrected report"
    },
    "icao": {
      "pattern": "^[A-Z]{4}$",
      "style": "location",
      "description": "ICAO location indicator (15.2)"
    },
    "datetime": {
      "pattern": "^\\d{6}Z$",
      "style": "datetime",
      "description": "Day and time UTC DDHHmmZ (15.3)"
    },
    "nil": {
      "pattern": "^NIL$",
      "style": "keyword",
      "description": "Missing report indicator"
    },
    "auto": {
      "pattern": "^AUTO$",
      "style": "keyword",
      "description": "Fully automated observation without human intervention (15.4)"
    },
    "wind": {
      "pattern": "^(VRB|\\d{3})(\\d{2,3})(G\\d{2,3})?(KT|MPS|KMH)$|^00000(KT|MPS|KMH)$|^P(49MPS|99KT)$",
      "style": "wind",
      "description": "Surface wind dddff[Gfmfm]{KT|MPS} (15.5)"
    },
    "windVariation": {
      "pattern": "^\\d{3}V\\d{3}$",
      "style": "wind",
      "description": "Variable wind direction dndndnVdxdxdx when variation ≥60° and <180° (15.5.3)"
    },
    "cavok": {
      "pattern": "^CAVOK$",
      "style": "visibility",
      "description": "Ceiling And Visibility OK - vis ≥10km, no cloud <1500m/MSA, no CB/TCU, no sig wx (15.10)"
    },
    "visibility": {
      "pattern": "^\\d{4}$|^9999$|^0000$",
      "style": "visibility",
      "description": "Prevailing visibility VVVV in metres (15.6)"
    },
    "directionalVisibility": {
      "pattern": "^\\d{4}(N|NE|E|SE|S|SW|W|NW)$",
      "style": "visibility",
      "description": "Minimum visibility VNVNVNVNDv with direction (15.6.2)"
    },
    "rvr": {
      "pattern": "^R\\d{2}[LCR]?\\/[PM]?\\d{4}(V[PM]?\\d{4})?[UDN]?$",
      "style": "visibility",
      "description": "Runway Visual Range RDRDR/VRVRVRVRi with P/M prefix and U/D/N tendency (15.7)"
    },
    "weather": {
      "pattern": "^[-+]?(VC)?(MI|PR|BC|DR|BL|SH|TS|FZ)?(DZ|RA|SN|SG|IC|PL|GR|GS|UP)*(BR|FG|FU|VA|DU|SA|HZ|PY)?(PO|SQ|FC|SS|DS)?$",
      "style": "weather",
      "description": "Present weather w'w' - intensity/proximity + descriptor + phenomena (15.8, Code table 4678)"
    },
    "weatherSpecial": {
      "pattern": "^(VCSH|VCTS|VCFG|VCPO|VCFC|VCDS|VCSS|VCBLDU|VCBLSA|VCBLSN|VCVA)$",
      "style": "weather",
      "description": "Weather phenomena in vicinity VC (15.8.10)"
    },
    "noWeather": {
      "pattern": "^\\/{2}$",
      "style": "weather",
      "description": "Present weather not observable by automatic system (15.8.19)"
    },
    "cloud": {
      "pattern": "^(FEW|SCT|BKN|OVC)(\\d{3})(CB|TCU|\\/\\/\\/)?$",
      "style": "cloud",
      "description": "Cloud layer NsNsNshshshs - amount + height in 100s ft + type (15.9.1)"
    },
    "cloudUnknown": {
      "pattern": "^(FEW|SCT|BKN|OVC)\\/\\/\\/(CB|TCU|\\/\\/\\/)?$",
      "style": "cloud",
      "description": "Cloud layer with unknown height (AUTO) (15.9.1.6)"
    },
    "verticalVisibility": {
      "pattern": "^VV(\\d{3}|\\/\\/\\/)$",
      "style": "cloud",
      "description": "Vertical visibility VVhshshs in 100s ft when sky obscured (15.9.2)"
    },
    "nsc": {
      "pattern": "^NSC$",
      "style": "cloud",
      "description": "No Significant Cloud - no cloud <1500m/MSA, no CB/TCU (15.9.1.1)"
    },
    "ncd": {
      "pattern": "^NCD$",
      "style": "cloud",
      "description": "No Cloud Detected by automatic observing system (15.9.1.1)"
    },
    "skyClear": {
      "pattern": "^(SKC|CLR)$",
      "style": "cloud",
      "description": "Sky Clear (regional usage)"
    },
    "temperature": {
      "pattern": "^M?\\d{2}\\/M?\\d{2}$",
      "style": "temperature",
      "description": "Temperature/Dewpoint T'T'/T'd T'd in °C, M for minus (15.11)"
    },
    "pressure": {
      "pattern": "^Q\\d{4}$",
      "style": "pressure",
      "description": "QNH pressure QPHPHPHPH in hPa (15.12)"
    },
    "pressureInches": {
      "pattern": "^A\\d{4}$",
      "style": "pressure",
      "description": "Altimeter setting in inches of mercury (regional - North America)"
    },
    "recentWeather": {
      "pattern": "^RE(MI|PR|BC|DR|BL|SH|TS|FZ)?(DZ|RA|SN|SG|IC|PL|GR|GS|UP)*(BR|FG|FU|VA|DU|SA|HZ|PY)?(PO|SQ|FC|SS|DS)?$",
      "style": "weather",
      "description": "Recent weather REw'w' since last report or last hour (15.13.2)"
    },
    "windshear": {
      "pattern": "^WS$",
      "style": "warning",
      "description": "Wind shear indicator (15.13.3)"
    },
    "windshearRunway": {
      "pattern": "^R\\d{2}[LCR]?$",
      "style": "warning",
      "description": "Wind shear runway designator"
    },
    "windshearAllRwy": {
      "pattern": "^ALL$",
      "style": "warning",
      "description": "Wind shear ALL RWY indicator"
    },
    "rwy": {
      "pattern": "^RWY$",
      "style": "warning",
      "description": "RWY keyword for WS ALL RWY"
    },
    "seaState": {
      "pattern": "^W(M?\\d{2})\\/S[0-9S]$",
      "style": "supplementary",
      "description": "Sea-surface temperature and state WTsTs/SS' (15.13.5)"
    },
    "waveHeight": {
      "pattern": "^W(M?\\d{2})\\/H\\d{3}$",
      "style": "supplementary",
      "description": "Sea-surface temperature and significant wave height WTsTs/HHsHsHs (15.13.5)"
    },
    "runwayState": {
      "pattern": "^R\\d{2}[LCR]?\\/([0-9\\/]{6}|CLRD\\/\\/|SNOCLO)$",
      "style": "supplementary",
      "description": "Runway state RDRDR/ERCReReRBRBR (15.13.6)"
    },
    "nosig": {
      "pattern": "^NOSIG$",
      "style": "trend",
      "description": "No significant change expected in next 2 hours (15.14)"
    },
    "becmg": {
      "pattern": "^BECMG$",
      "style": "trend",
      "description": "Becoming - change expected at regular/irregular rate (15.14.4)"
    },
    "tempo": {
      "pattern": "^TEMPO$",
      "style": "trend",
      "description": "Temporary fluctuations <1h each, <50% of period (15.14.7)"
    },
    "trendTime": {
      "pattern": "^(FM|TL|AT)\\d{4}$",
      "style": "datetime",
      "description": "Trend time indicator FM/TL/AT + HHmm (15.14.3)"
    },
    "trendWind": {
      "pattern": "^(VRB|\\d{3})(\\d{2,3})(G\\d{2,3})?(KT|MPS|KMH)$|^00000(KT|MPS|KMH)$",
      "style": "wind",
      "description": "Trend forecast wind"
    },
    "trendVisibility": {
      "pattern": "^\\d{4}$|^9999$|^CAVOK$",
      "style": "visibility",
      "description": "Trend forecast visibility"
    },
    "trendWeather": {
      "pattern": "^[-+]?(VC)?(MI|PR|BC|DR|BL|SH|TS|FZ)?(DZ|RA|SN|SG|IC|PL|GR|GS|UP)*(BR|FG|FU|VA|DU|SA|HZ|PY)?(PO|SQ|FC|SS|DS)?$",
      "style": "weather",
      "description": "Trend forecast weather"
    },
    "nsw": {
      "pattern": "^NSW$",
      "style": "weather",
      "description": "No Significant Weather - end of weather phenomena in trend (15.14.12)"
    },
    "trendCloud": {
      "pattern": "^(FEW|SCT|BKN|OVC)(\\d{3})(CB|TCU)?$|^VV(\\d{3}|\\/\\/\\/)$|^NSC$",
      "style": "cloud",
      "description": "Trend forecast cloud"
    },
    "remark": {
      "pattern": "^RMK$",
      "style": "remark",
      "description": "Remarks section indicator"
    },
    "remarkContent": {
      "pattern": "^[A-Z0-9\\/]+$",
      "style": "remark",
      "description": "Remark content (national use)"
    }
  },
  "sequence": [
    { "token": "identifier", "required": true },
    { "token": "correction", "required": false },
    { "token": "icao", "required": true },
    { "token": "datetime", "required": true },
    { "oneOf": [
      { "token": "nil", "terminal": true },
      { "sequence": [
        { "token": "auto", "required": false },
        { "token": "wind", "required": true },
        { "token": "windVariation", "required": false },
        { "oneOf": [
          { "token": "cavok" },
          { "sequence": [
            { "token": "visibility", "required": true },
            { "token": "directionalVisibility", "required": false },
            { "token": "rvr", "required": false, "repeat": true, "max": 4 },
            { "oneOf": [
              { "token": "weather", "required": false, "repeat": true, "max": 3 },
              { "token": "weatherSpecial", "required": false, "repeat": true, "max": 3 },
              { "token": "noWeather", "required": false }
            ]},
            { "oneOf": [
              { "token": "nsc" },
              { "token": "ncd" },
              { "token": "skyClear" },
              { "token": "verticalVisibility" },
              { "token": "cloud", "repeat": true, "max": 4 },
              { "token": "cloudUnknown", "repeat": true, "max": 4 }
            ]}
          ]}
        ]},
        { "token": "temperature", "required": true },
        { "oneOf": [
          { "token": "pressure", "required": true },
          { "token": "pressureInches", "required": true }
        ]},
        { "token": "recentWeather", "required": false, "repeat": true, "max": 3 },
        { "sequence": [
          { "token": "windshear", "required": false },
          { "oneOf": [
            { "token": "windshearRunway" },
            { "sequence": [{ "token": "windshearAllRwy" }, { "token": "rwy" }] }
          ]}
        ], "required": false },
        { "token": "seaState", "required": false },
        { "token": "waveHeight", "required": false },
        { "token": "runwayState", "required": false, "repeat": true },
        { "oneOf": [
          { "token": "nosig" },
          { "sequence": [
            { "oneOf": [{ "token": "becmg" }, { "token": "tempo" }] },
            { "token": "trendTime", "required": false, "repeat": true, "max": 2 },
            { "token": "trendWind", "required": false },
            { "oneOf": [
              { "token": "trendVisibility", "required": false },
              { "token": "cavok", "required": false }
            ]},
            { "oneOf": [
              { "token": "trendWeather", "required": false, "repeat": true, "max": 3 },
              { "token": "nsw", "required": false }
            ]},
            { "token": "trendCloud", "required": false, "repeat": true, "max": 4 }
          ], "repeat": true }
        ], "required": false },
        { "sequence": [
          { "token": "remark", "required": true },
          { "token": "remarkContent", "required": false, "repeat": true }
        ], "required": false }
      ]}
    ]}
  ],
