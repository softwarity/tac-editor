{
  "name": "SIGMET WV",
  "version": "1.0.0",
  "description": "SIGMET VA: Volcanic ash",
  "extends": "sigmet",
  "category": "WV",
  "suggestions": {
    "declarations": [
      {
        "id": "sigmet_wv_full",
        "ref": "identifier",
        "provider": "sigmet-wv-fir-location-indicator",
        "suffix": " SIGMET",
        "text": "AAAA SIGMET",
        "description": "SIGMET WV message (enter FIR code)",
        "placeholder": "AAAA SIGMET",
        "editable": {
          "start": 0,
          "end": 4
        }
      },
      {
        "id": "mt_prefix",
        "ref": "mt",
        "text": "MT",
        "description": "Mountain prefix"
      },
      {
        "id": "volcano_name",
        "ref": "volcanoName",
        "provider": "sigmet-va-volcano-name",
        "pattern": "[A-Z][A-Z\\-\\s]+",
        "description": "Volcano name",
        "placeholder": "VOLCANO",
        "editable": {
          "start": 0,
          "end": 7,
          "pattern": "[A-Z][A-Z\\-\\s]+",
          "description": "Volcano name"
        }
      }
    ],
    "after": {
      "start": ["sigmet_wv_full"],
      "fir": ["va_cld", "va_eruption"],
      "uir": ["va_cld", "va_eruption"],
      "firUir": ["va_cld", "va_eruption"],
      "cta": ["va_cld", "va_eruption"],
      "statusTest": ["va_cld", "va_eruption"],
      "statusExer": ["va_cld", "va_eruption"],
      "vaCld": ["mt_prefix", "volcano_name"],
      "vaEruption": ["mt_prefix", "volcano_name"],
      "mt": ["volcano_name"],
      "volcanoName": ["obs_kw", "fcst_kw"]
    }
  }
}
